<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="120.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.unvalidUrl.unvalidUrl">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="120.0" prefWidth="300.0">
         <children>
            <Label fx:id="Urlname" alignment="CENTER" prefHeight="17.0" prefWidth="265.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label text="This is unvalid Url, Search for better one">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Button fx:id="CloseBtn" mnemonicParsing="false" onAction="#CLoseIt" prefWidth="60.0" styleClass="cancelBtn" stylesheets="@../css/pasueGui.css" text="Close">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
